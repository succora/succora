<div class="well">
<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :subtitle %>
    <%= f.input :goal %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-success btn-lg" %>
  </div>
<% end %>
</div>


